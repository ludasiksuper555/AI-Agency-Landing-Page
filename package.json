{
  "name": "ai-agency-landing",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "quality:check": "npm run type-check && npm run lint && npm run format:check",
    "quality:fix": "npm run format && npm run lint:fix",
    "test": "jest",
    "stage0": "node scripts/stage0-preparation.js",
    "stage0:env": "node scripts/health-checks/validate-env.js",
    "stage0:services": "node scripts/health-checks/check-services.js",
    "stage0:git": "node scripts/git/git-status-check.js",
    "stage0:backup": "node scripts/backup/project-backup.js",
    "buffer:calculate": "node scripts/buffer-time/buffer-calculator.js",
    "fallback:generate": "node scripts/fallback/fallback-generator.js",
    "health:check": "ts-node scripts/monitoring/health-check-service.ts",
    "dev:health": "ts-node scripts/monitoring/health-check-service.ts --dev",
    "prestart": "npm run stage0",
    "prebuild": "npm run stage0:env && npm run stage0:git",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:pwa": "node automated-pwa-tests.js",
    "test:pwa:headless": "HEADLESS=true node automated-pwa-tests.js",
    "test:pwa:visible": "HEADLESS=false node automated-pwa-tests.js",
    "test:pwa:local": "TEST_URL=http://localhost:3000 node automated-pwa-tests.js",
    "test:pwa:staging": "TEST_URL=https://staging.yourdomain.com node automated-pwa-tests.js",
    "test:pwa:production": "TEST_URL=https://yourdomain.com node automated-pwa-tests.js",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "analyze": "cross-env ANALYZE=true next build",
    "clean": "rimraf .next out dist coverage",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "fix-ts": "node scripts/fix-typescript-issues.js",
    "check-all": "npm run type-check && npm run lint && npm run format:check && npm run test:ci",
    "build:analyze": "cross-env ANALYZE=true npm run build",
    "export": "next export",
    "build:prod": "NODE_ENV=production npm run build",
    "start:prod": "NODE_ENV=production npm start",
    "perf:lighthouse": "lighthouse http://localhost:3000 --output=html --output-path=./lighthouse-report.html",
    "perf:bundle": "npm run build:analyze",
    "optimize": "node scripts/optimize-performance.js"
  },
