# Telegram Bot Configuration
BOT_TOKEN=your_telegram_bot_token_here
ADMIN_TELEGRAM_ID=your_admin_telegram_id

# Freelance Platform APIs
UPWORK_API_KEY=your_upwork_api_key
UPWORK_API_SECRET=your_upwork_api_secret
FREELANCER_API_KEY=your_freelancer_api_key
FREELANCER_API_SECRET=your_freelancer_api_secret
FIVERR_API_KEY=your_fiverr_api_key

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key
OPENAI_MODEL=gpt-3.5-turbo

# Database Configuration
MONGODB_URI=mongodb://localhost:27017/techmoneybot
REDIS_URL=redis://localhost:6379

# Server Configuration
PORT=3000
NODE_ENV=development
WEBHOOK_DOMAIN=https://yourdomain.com
WEBHOOK_PATH=/webhook

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Search Configuration
MAX_PROJECTS_PER_DAY=100
MAX_PROPOSALS_PER_DAY=20
SEARCH_INTERVAL_HOURS=2

# Notification Settings
DAILY_REPORT_TIME=09:00
WEEKLY_REPORT_DAY=monday

# Security
JWT_SECRET=your_jwt_secret_key
ENCRYPTION_KEY=your_encryption_key

# Logging
LOG_LEVEL=info
LOG_FILE_PATH=./logs/bot.log

# External Services
PROXY_URL=
USER_AGENT=TechMoneyBot/1.0

# Feature Flags
ENABLE_AUTO_SEARCH=true
ENABLE_AUTO_PROPOSALS=false
ENABLE_ANALYTICS=true
ENABLE_NOTIFICATIONS=true
