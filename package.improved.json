{
  "name": "ai-agency-landing",
  "version": "0.1.0",
  "private": true,
  "description": "Modern AI Agency Landing Page built with Next.js, TypeScript, and Tailwind CSS",
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "keywords": ["nextjs", "typescript", "tailwindcss", "ai", "landing-page", "react"],
  "homepage": "https://your-domain.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/ai-agency-landing.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/ai-agency-landing/issues"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint:strict": "eslint --max-warnings 0 src",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "e2e:headed": "playwright test --headed",
    "analyze": "cross-env ANALYZE=true next build",
    "build-stats": "npx @next/bundle-analyzer",
    "clean": "rimraf .next out dist coverage",
    "prepare": "husky install",
    "postinstall": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:reset": "prisma migrate reset",
    "db:seed": "prisma db seed",
    "db:studio": "prisma studio",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "chromatic": "npx chromatic --project-token=your-project-token",
    "pre-commit": "lint-staged",
    "validate": "npm run typecheck && npm run lint:strict && npm run format:check && npm run test:ci",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major",
    "docker:build": "docker build -t ai-agency-landing .",
    "docker:run": "docker run -p 3000:3000 ai-agency-landing"
  },
